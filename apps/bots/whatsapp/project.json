{
  "name": "bot-whatsapp",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/bots/whatsapp/src",
  "projectType": "application",
  "tags": ["type:app", "platform:whatsapp"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm build",
        "cwd": "apps/bots/whatsapp"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm dev",
        "cwd": "apps/bots/whatsapp"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm start",
        "cwd": "apps/bots/whatsapp"
      }
    }
  }
}
